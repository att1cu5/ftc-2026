package org.firstinspires.ftc.teamcode;
import org.firstinspires.ftc.robotcore.external.hardware.camera.controls.CameraControl
import java.io.Serializable 
import java.lang.Comparable
import org.firstinspires.ftc.robotcore.external.ClassFactory;
import org.firstinspires.ftc.robotcore.external.hardware.camera.WebcamName;
import org.firstinspires.ftc.robotcore.external.navigation.VuforiaLocalizer;
import org.firstinspires.ftc.robotcore.external.hardware.camera.controls.FocusControl;
import com.qualcomm.hardware.rev.RevHubOrientationOnRobot;
import com.qualcomm.robotcore.eventloop.opmode.LinearOpMode;
import com.qualcomm.robotcore.eventloop.opmode.TeleOp;
public enum FocusControl.Mode{
  Auto,
  Fixed,
  Unknown,
  ContinousAuto,
  Macro,
  Infinty
}
public enum WhiteBalanceControl.Mode {
  Auto
}
public enum ExposureControl.Mode{
  Unknown,
  Auto,
  ContinousAuto,
  Maunal,
  ShutterPriorty,
  AutoPriority
}
ExposureControl MyExposureControl;
@TeleOp (name = "Camera (Blocks to Java)")
public class Camera extends LinearOpMode {

}
@Override
public void runOpMode() {
       private static final String VUFORIA_KEY =
            "   INSERT YOUR VUFORIA KEY HERE  ";

    // Class Members
        private VuforiaLocalizer vuforia    = null;
        private WebcamName webcamName       = null;
        myExposureControl = vuforia.getCamera().getControl(ExposureControl.class);  
        myExposureControl.setMode(ExposureControl.Mode.Manual);
        waitForStart();
        
        while (opModeIsActive()) {
           myExposureControl.setExposure(30, TimeUnit.MILLISECONDS);

 }

}







